---
import "../styles/index.css";
import CommonLayout from "../layouts/common.astro";
import Items from "../components/Items.astro";
import Logo from "../components/Logo.astro";
import { getCollection } from "astro:content";

const entries = await getCollection("demos");

const items = entries
  //.filter((e) => !e.id.match(/./))
  .sort((a, b) => a.data.title.localeCompare(b.data.title))
  .map((e) => ({
    href: `/demos/${e.data.mapId}/index.html`,
    title: e.data.title,
  }));
---

<CommonLayout title="svgmapviewer">
  <Logo />
  <Items items={items} />
</CommonLayout>
