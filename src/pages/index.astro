---
import CommonLayout from "../layouts/common.astro";
import Items from "../components/Items.astro";
import logo from "../images/logo.svg";
import { Image } from "astro:assets";
import { getCollection } from "astro:content";

const entries = await getCollection("demos");

console.log(entries);

const items = entries
  //.filter((e) => !e.id.match(/./))
	.map((e) => ({
		href: `/demos/${e.data.title}/index.html`,
		title: e.data.title,
	}));
---

<CommonLayout title="svgmapviewer">
  <Image src={logo} class="logo" alt="logo" />
	<Items items={items} />
</CommonLayout>
